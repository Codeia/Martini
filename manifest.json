{
    "name": "Martini",
    "description": "Chrome extension that brings helpful tools for CSGO Traders",
    "homepage_url": "https://github.com/Codeia/Martini",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "*://*/"
    ],
    "icons": { "16": "icons/16.png", "48": "icons/48.png", "128": "icons/128.png" },
    "content_scripts": [
        {
            "matches": ["*://steamcommunity.com/*/*"],
            "js": [
                "jquery.min.js",
                "js/qio/qio.js"
            ]
        }
    ]

}
